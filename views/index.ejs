<% include partials/header %>
    <h1>Add customer</h1>
    <form method="POST" action="/users/add">
        <label>First name</label>
        <input type="texr" name="first_name">
        <br>
        <label>Last name</label>
        <input type="texr" name="last_name">
        <br>
        <label>Email</label>
        <input type="texr" name="email">
        <br><br>
        <input type="submit" value="Submit">
    </form>
    <h1>
        <%= title %>
    </h1>
    <ul>
        <% users.forEach(function(user){ %>
            <li>
                <%= user.first_name  %>
                    <%= user.last_name  %> - <a href="#" class="deleteUser" data-id="<%= user._id %>">Delete</a>
            </li>
            <% }) %>
    </ul>
    <% include partials/footer %>